<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Puzzlr</name>

    <description>
        Final Puzzle Game App built for Interactive Coding. 
    </description>

    <author href="https://build.phonegap.com" email="mpatterson12@live.ca">
        Mitchell Patterson 
    </author>

    <platform name="android">
        <icon src="img/icon.png" />
        <icon gap:density="ldpi" gap:platform="android" src="img/icon.png" />
        <icon gap:density="mdpi" gap:platform="android" src="img/icon.png" />
        <icon gap:density="hdpi" gap:platform="android" src="img/icon.png" />
        <icon gap:density="xhdpi" gap:platform="android" src="img/icon.png" />
    </platform>

</widget>